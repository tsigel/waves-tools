{
  "name": "@waves-tools/fetch",
  "version": "0.0.0",
  "description": "Request with node-fetch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.0.27",
    "node-fetch": "^2.6.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "jest": "^26.3.0",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "prepublish": "yarn test",
    "postpublish": "git push",
    "postversion": "yarn publish",
    "test": "jest",
    "build": "npx tsc"
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__test__/*spec.+(ts)"
    ]
  }
}
